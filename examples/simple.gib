
keyword select;
keyword from;
keyword delete;

token NUM = "[0-9]+";
token STR = "\"[^\"]*\"";
token WHITESPACE = "\s+";
token IDENT = "[_a-zA-Z]+";
token COLON = ":";
token COMMA = ",";
token SEMI = ";";
token PLUS = "\+";
token TIMES = "\*";
token LParen = "\(";
token RParen = "\)";

parser string = STR;
parser num = NUM;
parser literal = num | string;
parser stmt = (select + literal + from + IDENT).skip(WHITESPACE)
